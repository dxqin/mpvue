.container-middle{
  width: 100%;
  height: 100%; 
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
}
.container-middle .content{
  width: 614rpx;
  height: 514rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.container-middle .content img{
  width: 426rpx;
  height: 318rpx;
  /* margin-top: 32rpx; */
  margin-bottom: 41rpx;
}
.container-middle .content span{
  height:34rpx;
  font-size:24rpx;
  color:rgba(59,59,59,1);
  line-height:34rpx;
}