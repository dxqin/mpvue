  height: 886rpx;
  margin-top: 72rpx;
  margin-bottom: 44rpx;
  box-shadow:0px 2rpx 8rpx 0px rgba(0,0,0,0.13);
  border-radius:10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.container-middle .qrcode .logo{
  width: 152rpx;
  height: 126rpx;
  margin-top: 62rpx;
  margin-bottom: 61rpx;
}
.container-middle .qrcode .qrcode-img-s{
  width: 532rpx;
  height: 532rpx;
  position: relative;
}
.container-middle .qrcode .avatar{
  position: absolute;
  /* top: 192rpx;
  bottom: 192rpx;
  left: 192rpx;
