  top: 215rpx;
  bottom: 215rpx;
  left: 215rpx;
  right: 215rpx;
  width: 100rpx;
  height: 100rpx;
  border-radius:10rpx;
  border:6rpx solid rgba(255,255,255,1);
}
.container-middle .qrcode .qrcode-img{
  width: 532rpx;
  height: 532rpx; 
  /* width: 486rpx;
  height: 486rpx;  
  margin-bottom: 46rpx;*/
}
.container-middle .qrcode span{
  /* width:492rpx; */
  height:34rpx;
  font-size:24rpx;
  font-family:PingFangSC-Regular;
  color:rgba(59,59,59,1);
  line-height:34rpx;
}
.container-middle button{
  width: 678rpx;
  height: 100rpx;
  border: 0;
  background-color: #00A4A2;
